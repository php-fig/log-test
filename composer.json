{
  "name": "fig/log-test",
  "description": "Test utilities for the psr/log package that backs the PSR-3 specification.",
  "license": "MIT",
  "authors": [
    {
      "name": "PHP-FIG",
      "homepage": "https://www.php-fig.org/",
      "role": "Organisation"
    }
  ],
  "require": {
    "php": "^8.0",
    "psr/log": "^2.0 | ^3.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.0 | ^9.0",
    "squizlabs/php_codesniffer": "^3.6"
  },
  "autoload": {
    "psr-4": {
      "Psr\\Log\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Fig\\Log\\Tests\\": "tests"
    }
  }
}
